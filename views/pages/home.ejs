<div class="row">
  
  <div class="col">
    <div class="">
      <ul class="list-group">
        <% allProvinces.map(prov => { %>
          <li class="list-group-item d-flex justify-content-between align-items-center">
            <%= prov %> 
            <span class="badge badge-primary badge-pill">14</span>
          </li>
        <% }) %>     
      </ul>
    </div>
  </div>

  <div class="col">
    <!-- Some text here -->
    <h1>Total Dengue: <%= totalDengue %> </h1>
    <h1>Total Zika: <%= totalZika %> </h1>
    <h1>Provincias: <%= allProvinces[0] %>, <%= allProvinces[1] %>, <%= allProvinces[2] %> </h1>
    <h1>Total provincias: <%= allProvinces.length %> </h1>
    <h1>All departaments: <%= allDepartaments[1] %> </h1>
  </div>

</div>